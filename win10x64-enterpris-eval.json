{ "_comment":"Modified for building window 10 amd64 client desktop with qemue.",

  "builders": [
    {
      "communicator": "winrm",
      "disk_size": "{{ user `disk_size` }}",
      "memory":"{{ user `memory` }}",
      "cpus":"{{ user `cpus` }}",
      "floppy_files": [
        "floppy/00-run-all-scripts.cmd",
        "floppy/01-install-wget.cmd",
        "floppy/_download.cmd",
        "floppy/_packer_config.cmd",
        "floppy/disablewinupdate.bat",
        "floppy/fixnetwork.ps1",
        "floppy/install-winrm.cmd",
        "floppy/openssh.bat",
        


        "floppy/zz-start-sshd.cmd",
        "floppy/eval-win10x64-enterprise/Autounattend.xml"
      ],
      "headless": "{{ user `headless` }}",
      "iso_checksum": "{{ user `iso_checksum` }}",
      "iso_checksum_type": "{{ user `iso_checksum_type` }}",
      "iso_url": "{{ user `iso_url` }}",
      "shutdown_command": "{{ user `shutdown_command` }}",
      "output_directory":"box/output-{{ user `vm_name` }}",
      "type": "qemu",
      "vm_name": "{{ user `vm_name` }}",
      "boot_wait": "2s",
      "net_device": "{{ user `net_device` }}",
      "disk_interface": "{{ user `disk_interface` }}",
      "winrm_username": "vagrant",
      "winrm_password": "vagrant",
      "winrm_timeout": "10000s",
      "qemuargs": [
        [ "-cpu", "{{ user `cpu_model`}}" ],
        [ "-m", "{{ user `memory` }}" ],
        [ "-smp", "cpus={{ user `cpus` }}"  ]
      ]
    }
  ],

  "_comment":"this is the postprocess section",

  "post-processors": [
    {
      "compression_level": 1,
      "keep_input_artifact": false,
      "output": "box/{{.Provider}}/{{user `vm_name`}}-vagrant-{{user `version`}}.box",
      "type": "vagrant",
      "vagrantfile_template": "tpl/vagrantfile-eval-win10x64-enterprise.tpl"
    }
  ],

  "_comment":"this is the provisioners",

  "provisioners": [
    {
      "environment_vars": [
        "CM={{user `cm`}}",
        "CM_VERSION={{ user `cm_version` }}",
        "UPDATE={{ user `update` }}"
      ],
      "scripts": [
        "script/vagrant.bat",
        "script/clean.bat",
        "script/ultradefrag.bat",
        "script/sdelete.bat"
      ],
      "type": "windows-shell"
    }
  ],

  "_comment":"these are the default variables for the above",

  "variables": {

    "cm": "chef",
    "cm_version": "0",
    "disk_size": "65000",
    "disk_interface": "ide",
    "net_device": "rtl8139",
    "vm_name": "eval-win10x64-enterprise",
    "memory":"2096",
    "cpus":"1",
    "cpu_model": "Westmere",
    "headless": "false",
    "iso_checksum_type": "sha1",
    "iso_checksum": "164f022a22d2305e6fbd82f6e35d530b6bbadca8",
    "iso_url": "/mnt/samdisk/ISO.Dir/window10_enterprise_client_eval_x64_en-us.iso",
    "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
    "update": "true",
    "version": "0.1.0"
  }
}
