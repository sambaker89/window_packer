{ "_comment":"Modified for building with qemue.",
  "builders": [
    {
      "communicator": "winrm",
      "disk_size": "{{user `disk_size`}}",
      "memory":"{{user `memory`}}",
      "cpus":"{{user `cpus`}}",
      "floppy_files": [
        "floppy/00-run-all-scripts.cmd",
        "floppy/01-install-wget.cmd",
        "floppy/_download.cmd",
        "floppy/_packer_config.cmd",
        "floppy/disablewinupdate.bat",
        "floppy/eval-win10x64-enterprise/Autounattend.xml",
        "floppy/fixnetwork.ps1",
        "floppy/install-winrm.cmd",
        "floppy/passwordchange.bat",
        "floppy/powerconfig.bat",
        "floppy/update.bat",
        "floppy/zz-start-sshd.cmd"
      ],

      "headless": "{{ user `headless` }}",
      "iso_checksum": "{{ user `iso_checksum` }}",
      "iso_checksum_type": "sha1",
      "iso_url": "{{ user `iso_url` }}",
      "shutdown_command": "{{ user `shutdown_command`}}",
      "output_directory":"box/",
      "type": "qemu",
      "format": "qcow2",
      "vm_name": "eval-win10x64-enterprise",
      "boot_wait": "2s",
      "accelerator": "kvm",
      "disk_interface": "virtio",

      "winrm_password": "vagrant",
      "winrm_timeout": "10000s",
      "winrm_username": "vagrant"
    },


  "provisioners": [
    {
      "scripts": [
        "script/vagrant.bat",
        "script/cmtool.bat",
        "script/vmtool.bat",
        "script/clean.bat",
        "script/ultradefrag.bat",
        "script/sdelete.bat"
      ],
      "type": "windows-shell"
    }
  ],
  "variables": {
    "cm": "chef",
    "cm_version": "",
    "disk_size": "40960",
    "memory":"2096",
    "cpus":"1",
    "headless": "false",
    "iso_checksum": "743fc483bb8bf1901c0534a0ae15208a5a72a3c5",
    "iso_url": "/mnt/mymedia/18362.30.190401-1528.19h1_release_svc_refresh_CLIENTENTERPRISEEVAL_OEMRET_x64FRE_en-us.iso",
    "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
    "update": "true",
    "version": "0.1.0"
  }
